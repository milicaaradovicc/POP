<Window x:Class="WPF.Profil.Automobili.NabavkaInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF.Profil.Automobili"
        mc:Ignorable="d"
        Title="Informacije o nabavci" Height="300" Width="347">
    <Grid>
        <StackPanel Margin="20">
            <TextBlock Text="Cena nabavke:" FontWeight="Bold"/>
            <TextBlock x:Name="CenaNabavkeTextBlock" Margin="0,5,0,10"/>
            <TextBlock Text="Datum nabavke:" FontWeight="Bold"/>
            <TextBlock x:Name="DatumNabavkeTextBlock" Margin="0,5,0,10"/>
            <TextBlock Text="Cena prodaje:" FontWeight="Bold"/>
            <TextBlock x:Name="CenaProdajeTextBlock" Margin="0,5,0,10"/>
        </StackPanel>
    </Grid>
</Window>
